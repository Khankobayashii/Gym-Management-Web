@model Project_63134295.Models.GoiTap

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
    
}

<h4 style="color:red;"> Bạn có chắc chắn xóa thông tin này?</h4>
<table border="0" cellpadding="0" cellspacing="0" style="font-size:14px;margin-left:50px;">
    <tr>
        <td>
            <table cellpadding="2" cellspacing="10">
                <caption><b>THÔNG TIN CHI TIẾT</b></caption>
                <tr>
                    <td>Mã gói:</td>
                    <td>@Html.DisplayFor(model => model.MaGoi)</td>
                </tr>
                <tr>
                    <td>Tên gói:</td>
                    <td>@Html.DisplayFor(model => model.TenGoi)</td>
                </tr>
                <tr>
                    <td>Giá:</td>
                    <td>@Html.DisplayFor(model => model.Gia)</td>
                </tr>
     
            </table>
        </td>
    </tr>
</table>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-actions no-color" style="margin-left:50px;">
        <input type="submit" value="Xóa" class="btn btn-danger" /> |
        @Html.ActionLink("Quay lại", "Index")
    </div>
}
